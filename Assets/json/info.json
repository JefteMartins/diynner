  {
      
    "id": 1,
    "name": "Arroz à grega",
    "image": "https://img.itdg.com.br/tdg/images/recipes/000/088/750/352590/352590_original.jpg?w=1200",
    "preparation":[
      {
        "step":1,
        "description":"Refogue a cebola e o alho na margarina."
      },
      {
        "step":2,
        "description":"Junte o arroz e refogue até secar bem."
      },
      {
        "step":3,
        "description":"Junte os legumes picados e a água com o caldo de galinha dissolvido."
      },
      {
        "step":4,
        "description":"Quando ferver, ajuste o sal."
      },
      {
        "step":5,
        "description":"Tampe e deixe cozinhar em fogo baixo."
      },
      {
        "step":6,
        "description":"Desligue o fogo quando secar."
      },
      {
        "step":7,
        "description":"Deixe o arroz na panela, descansando, por mais cerca de 5 minutos."
      }
      
    ]
    ,
